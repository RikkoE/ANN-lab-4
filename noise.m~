function [ output_args ] = noise( input_args )

pict;
w = p1'*p1 + p2'*p2 + p3'*p3;
iter = ;
input = p1;

for i = 1:iter
    pdist = flip(input,i);
    
    vis(pdist);
    pdist = sgn(w*pdist')';
    
    
    
%     if isequal(pdist, input) == 0
%         disp('not equal at iteration: ');
%         disp(iter);
%         vis(pdist);
%     end
end

end

