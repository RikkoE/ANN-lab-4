function [ output_args ] = sequpdate( input_args )
%SEQUPDATE Summary of this function goes here
%   Detailed explanation goes here
pict;
w = p1'*p1 + p2'*p2 + p3'*p3;

iter = 2000;
unit = randi([1,1024], iter, 1);

%     vis(p22);
% vis(sgn(w * p11')');

% disp(p11');

for i = 1:iter
    
    
    pattern = sgn(w(,unit(i)) * p11(unit(i))');
    
end

end

